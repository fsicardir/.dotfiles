
cmd - return: alacritty

cmd + shift - return: alacritty -e tmux

# focus window
cmd - h : yabai -m window --focus west || yabai -m display --focus west
cmd - j : yabai -m window --focus south || yabai -m display --focus south
cmd - k : yabai -m window --focus north || yabai -m display --focus north
cmd - l : yabai -m window --focus east || yabai -m display --focus est

# swap managed window
cmd + shift - h : yabai -m window --swap west
cmd + shift - j : yabai -m window --swap south
cmd + shift - k : yabai -m window --swap north
cmd + shift - l : yabai -m window --swap east

# move managed window
cmd + alt - h : yabai -m window --warp west
cmd + alt - j : yabai -m window --warp south
cmd + alt - k : yabai -m window --warp north
cmd + alt - l : yabai -m window --warp east

# make floating window fill screen
cmd + shift - up     : yabai -m window --grid 1:1:0:0:1:1
# center floating window
cmd + shift - down   : yabai -m window --grid 4:4:1:1:2:2
# use west space
cmd + shift - left   : yabai -m window --grid 1:2:0:0:1:1
# use east space
cmd + shift - right  : yabai -m window --grid 1:2:1:1:1:1

# send window to desktop
cmd + shift - 1 : yabai -m window --space  1
cmd + shift - 2 : yabai -m window --space  2
cmd + shift - 3 : yabai -m window --space  3
cmd + shift - 4 : yabai -m window --space  4
cmd + shift - 5 : yabai -m window --space  5

cmd - tab : yabai -m display --focus next

# float / unfloat window and center on screen
cmd - f : yabai -m window --toggle float;\
          yabai -m window --grid 4:4:1:1:2:2
